{
  "name": "cargo-bloat-action",
  "version": "0.0.0",
  "private": true,
  "description": "Track bloat in your Rust projects",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "pack": "ncc build",
    "build-release": "pnpm run build && pnpm run pack",
    "all": "pnpm run build && pnpm run pack",
    "add-release": "git add dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orf/cargo-bloat-action.git"
  },
  "pre-commit": [
    "build-release",
    "add-release"
  ],
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "orf",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.3",
    "axios": "^1.4.0",
    "diff": "^5.1.0",
    "filesize": "^10.0.7",
    "lodash": "^4.17.21",
    "text-table": "^0.2.0",
    "treeify": "^1.1.0"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.34",
    "@types/text-table": "^0.2.2",
    "@types/treeify": "^1.0.0",
    "@vercel/ncc": "^0.36.1",
    "js-yaml": "^4.1.0",
    "pre-commit": "^1.2.2",
    "typescript": "^5.0.4"
  }
}
